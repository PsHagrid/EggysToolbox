{
  "manifest_version": 3,
  "name": "Aiman Toolbox",
  "description": "A collection of tools made by Aiman, including Chat Exporter.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "web_accessible_resources": [{
    "resources": ["jszip.min.js"],
    "matches": ["<all_urls>"]
  }],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
